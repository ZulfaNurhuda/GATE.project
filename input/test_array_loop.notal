Program TestArrayLoop;

KAMUS
  myNumbers : array [1..5] of integer;
  i : integer;
  temp : integer; // Declared but not used, for testing declaration

ALGORITMA
  // Initialize array
  for i <- 1 to 5 do
    myNumbers[i] <- i * 10;

  // Output array elements
  output("Array elements:");
  for i <- 1 to 5 do
    output("Element ", i, ": ", myNumbers[i]);

  // Test input to an array element
  output("Enter a new value for element 3:", INLINE)
  input(myNumbers[3]);
  output("New element 3 is: ", myNumbers[3]);

END.
